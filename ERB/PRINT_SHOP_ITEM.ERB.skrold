;==========================================================================
;衣服補正表示
;==========================================================================
@INDICATE_CLOTHE
#DIM hosei, 1

PRINT 　
FOR COUNT, 0, 10
	CALL PALAM_CONVERT, COUNT
	IF COUNT <= 5 || COUNT >= 8
		LOCAL = 0
		
		;布切れ系統はIDがマイナスのため、関数名が独立している
		IF CFLAG:服装 < 0
			STR:1 = REV
		ELSE
			STR:1 = 
		ENDIF
		
		;布切れ系統はIDがマイナスのため、関数名が独立している
		IF CFLAG:服装 < 0
			CALLFORM CLOTHE_CLOTH_{CFLAG:服装*-1}, STR
		ELSE
			CALLFORM CLOTHE_ID_{CFLAG:服装}, STR
		ENDIF
		LOCAL = RESULT
		CALLFORM REMODEL_HOSEI, STR
		LOCAL += RESULT
		IF LOCAL
			PRINTFORM %STR%:
			
			;改造して属性が付与されている場合、色を変える
			IF RESULT >= 1
				CALL SETCOLOR_PALAMUP
			ELSEIF RESULT <= -1
				CALL SETCOLOR_PALAMDOWN
			ENDIF
			
			IF LOCAL < 0
				FOR COUNT:1, 0, -1*LOCAL
					PRINT -
				NEXT
			ELSE
				FOR COUNT:1, 0, LOCAL
					PRINT +
				NEXT
			ENDIF
			RESETCOLOR
			PRINT 　
		ENDIF
	ENDIF
NEXT

;布切れ系統はIDがマイナスのため、関数名が独立している
IF CFLAG:服装 < 0
	CALLFORM CLOTHE_CLOTH_{CFLAG:服装*-1}, "姉妹への友好度"
ELSE
	CALLFORM CLOTHE_ID_{CFLAG:服装}, "姉妹への友好度"
ENDIF
