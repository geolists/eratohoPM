;================================================
;販売アイテム表示
;ARG　表示するアイテムの種類
;================================================
;店主の台詞、購入処理等は「ITEM.ERB」に格納されています


@PRINT_SHOP_ITEM, ARG
#DIM WORD, 2
#DIM kosuu, 1
#DIM kyosei, 2
#DIM kanraku, 1
#DIM L_LOCAL, 1
#DIM check, 1
#DIM ITEM_LIST, 6

$BACK_LOOP
DRAWLINE
PRINTFORML 何を購入しますか？
PRINTFORM 現在の所持金(
CALL SETCOLOR_MONEY
PRINTFORM ${MONEY}
RESETCOLOR
PRINTL )
DRAWLINE
;1:　衣装販売
IF ARG == 1
	;通常の衣装
	REPEAT 6
		LOCAL = COUNT + 1
		
		IF LOCAL == 6
			LOCAL = 8
		ENDIF
		
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;姫メイドレスは姫様属性とメイド属性を50％ずつ持っていないと購入できない
		ELSEIF LOCAL == 8 && (CFLAG:属性／メイド < 50 || CFLAG:属性／姫様 < 50)
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,26,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	REND
	
	;コスプレ用
	;それぞれ経験が高くないと売り出されない
	;普段着を持っていない場合も解禁される
	REPEAT 7
		LOCAL = COUNT + 10
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;メイド服は家事経験が20以上で売り出される
		ELSEIF LOCAL == 10 && EXP:家事経験 < 20 && ITEM:0
			ITEMSALES:LOCAL = 0
		;修道服は信仰経験が20以上で売り出される
		ELSEIF LOCAL == 11 && EXP:信仰経験 < 20 && ITEM:0
			ITEMSALES:LOCAL = 0
		;ゴスロリは知力経験が20以上で売り出される
		ELSEIF LOCAL == 12 && EXP:知力経験 < 20 && ITEM:0
			ITEMSALES:LOCAL = 0
		;ドレスは背徳経験が20以上で売り出される
		ELSEIF LOCAL == 13 && EXP:背徳経験 < 20 && ITEM:0
			ITEMSALES:LOCAL = 0
		;呪詛の布切れは普段着を持っていない場合のみ購入可
		ELSEIF LOCAL == 14 && ITEM:0
			ITEMSALES:LOCAL = 0
		;各種道着は体力経験が20以上で売り出される
		ELSEIF (LOCAL == 15 || LOCAL == 16) && EXP:体力経験 < 20 && ITEM:0
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,26,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			
			;呪詛の布切れ以外は値段を5倍にする
			IF LOCAL != 14
				A *= 5
			ENDIF
			
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	REND
	
	;中級コスプレ服
	;それぞれの割合が15％を超えると売り出される
	REPEAT 7
		IF COUNT == 4
			COUNT += 1
		ENDIF
		
		LOCAL = COUNT + 20
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;メイド服属性が15％必要
		;以下、同様の条件で解禁
		ELSEIF LOCAL == 20 && CFLAG:属性／メイド < 15
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 21 && CFLAG:属性／シスター < 15
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 22 && CFLAG:属性／ゴスロリ < 15
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 23 && CFLAG:属性／花嫁 < 15
			ITEMSALES:LOCAL = 0
		ELSEIF (LOCAL == 25 || LOCAL == 26) && CFLAG:属性／武士 < 15
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,26,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	REND
	
	;上級コスプレ服
	;それぞれの割合が30％を超えると売り出される
	REPEAT 7
		IF COUNT == 4
			COUNT += 1
		ENDIF
		
		LOCAL = COUNT + 30
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;メイド服属性が30％必要
		;以下、同様の条件で解禁
		ELSEIF LOCAL == 30 && CFLAG:属性／メイド < 30
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 31 && CFLAG:属性／シスター < 30
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 32 && CFLAG:属性／ゴスロリ < 30
			ITEMSALES:LOCAL = 0
		ELSEIF LOCAL == 33 && CFLAG:属性／花嫁 < 30
			ITEMSALES:LOCAL = 0
		ELSEIF (LOCAL == 35 || LOCAL == 36) && CFLAG:属性／武士 < 30
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,26,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	REND
	
	LOCAL = 50
	
	;その他衣装
	;それぞれに条件がある
	FOR L_LOCAL, 50, 71
		IF L_LOCAL == 52
			L_LOCAL = 53
		ELSEIF L_LOCAL == 55
			L_LOCAL = 56
		ELSEIF L_LOCAL == 61
			L_LOCAL = 65
		ELSEIF L_LOCAL == 66
			L_LOCAL = 69
		ENDIF
		LOCAL = L_LOCAL
		
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;ブレザーは知力60以上でないと売り出されない
		ELSEIF LOCAL == 50 && BASE:知力 < 60
			ITEMSALES:LOCAL = 0
		;ワンピース制服は知力80でないと売り出されない
		ELSEIF LOCAL == 51 && BASE:知力 < 80
			ITEMSALES:LOCAL = 0
		;特殊修道服はシスター属性が20％以上ないと売り出されない
		ELSEIF (LOCAL == 53 || LOCAL == 54) && CFLAG:属性／シスター < 20
			ITEMSALES:LOCAL = 0
		;裸エプロンは花嫁属性が20％以上ないと売り出されない
		ELSEIF LOCAL == 56 && CFLAG:属性／花嫁 < 20
			ITEMSALES:LOCAL = 0
		;エプロンメイドはメイド属性が30％以上ないと売り出されない
		ELSEIF LOCAL == 57 && CFLAG:属性／メイド < 30
			ITEMSALES:LOCAL = 0
		;エプロンドレスは花嫁属性が20％以上ないと売り出されない
		ELSEIF LOCAL == 58 && CFLAG:属性／花嫁 < 20
			ITEMSALES:LOCAL = 0
		;姫巫女装束は姫様属性とシスター属性を50％ずつ持っていないといけない
		ELSEIF LOCAL == 59 && (CFLAG:属性／シスター < 50 || CFLAG:属性／姫様 < 50)
			ITEMSALES:LOCAL = 0
		;ゴスロリメイド服はメイド属性とゴスロリ属性を50％ずつ持っていないといけない
		ELSEIF LOCAL == 60 && (CFLAG:属性／メイド < 50 || CFLAG:属性／ゴスロリ < 50)
			ITEMSALES:LOCAL = 0
		;姉妹の書生服は[姉妹の許可]が無いと販売されない
		ELSEIF LOCAL == 65 && !TALENT:姉妹の許可
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,26,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL += 1
	NEXT

ENDIF

;2:　娼婦用衣装
IF ARG == 2
	REPEAT 4
		LOCAL = COUNT + 100
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,20,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	REND
ENDIF

;3:　裏アイテム
IF ARG == 3
	LOCAL = 496
	REPEAT 10
		IF LOCAL == 498
			LOCAL = 500
		ELSEIF LOCAL == 502
			LOCAL = 506
		ELSEIF LOCAL == 508
			LOCAL = 527
		ELSEIF LOCAL == 528
			LOCAL = 529
		ELSEIF LOCAL == 530
			LOCAL = 537
		ELSEIF LOCAL == 538
			DRAWLINE
			LOCAL = 545
		ENDIF
		
		IF ITEM:LOCAL
			ITEMSALES:LOCAL = 0
		;ロープ付き首輪は隷属が入ってないと売られない
		ELSEIF LOCAL == 506 && !BASE:隷属
			ITEMSALES:LOCAL = 0
		;天使の施しは隷属が1000以上入ってないと売られない
		;2013/08/28　販売中止。再販は恐らく2ヶ月近く後になるでしょう
		;2013/09/01　名称変更しました(天使の施し→インコヒーレンス)。販売再開
		ELSEIF LOCAL == 527 && BASE:隷属 < 1000
			ITEMSALES:LOCAL = 0
		ELSE
			ITEMSALES:LOCAL = 1
			PRINTFORM [{LOCAL,3}] - %ITEMNAME:LOCAL,20,LEFT%
			BOUGHT = LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL += 1
	REND
ENDIF

;4:　装飾品
IF ARG == 4
	LOCAL:1 = 400
	REPEAT 6
		IF LOCAL:1 == 402
			LOCAL:1 = 410
		ELSEIF LOCAL:1 == 412
			LOCAL:1 = 420
		ENDIF
		
		IF ITEM:(LOCAL:1)
			ITEMSALES:(LOCAL:1) = 0
		ELSE
			ITEMSALES:(LOCAL:1) = 1
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
			BOUGHT = LOCAL:1
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL:1 += 1
	REND
	DRAWLINE
	
	LOCAL:1 = 428
	;救済の書
	IF !ITEM:(LOCAL:1)
		ITEMSALES:(LOCAL:1) = 1
		PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
		BOUGHT = LOCAL:1
		CALL SETCOLOR_MONEY
		CALL ITEM_MONEY
		PRINTFORML ${A,6}
		RESETCOLOR
	ENDIF
	
	;入浴剤
	FOR L_LOCAL, 490, 496
		IF ITEM:L_LOCAL
			ITEMSALES:L_LOCAL = 0
		ELSE
			ITEMSALES:L_LOCAL = 1
			PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:(L_LOCAL),20,LEFT%
			BOUGHT = L_LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	NEXT
ENDIF

;5:　技巧
IF ARG == 5
	LOCAL:1 = 350
	REPEAT 3
		IF TALENT:(LOCAL:1)
			ITEMSALES:(LOCAL:1) = 0
		ELSE
			ITEMSALES:(LOCAL:1) = 1
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
			BOUGHT = LOCAL:1
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL:1 += 1
	REND
ENDIF

;6:　裏装飾品
IF ARG == 6
	LOCAL:1 = 503
	
	kyosei = 0
	kyosei:1 = 0
	kanraku = 0
	
	;**ここからPetitPatch4**
	;大きく書き換えるので原型保持
	;FOR COUNT, 2, 2+FLAG:攫った姉妹の数
	;	IF GETBIT(CFLAG:COUNT:矯正アイテム, 0)
	;		kyosei += 1
	;	ENDIF
	;	
	;	IF GETBIT(FLAG:姉妹陥落フラグ, COUNT+1)
	;		kanraku += 1
	;	ENDIF
	;	
	;	IF GETBIT(CFLAG:COUNT:矯正アイテム, 1)
	;		kyosei:1 += 1
	;	ENDIF
	;NEXT
	
	;里娘の分を考慮する必要がある
	;とりあえず素質で判断するように
	;CHARANUM使えば落ちないよね
	FOR COUNT, 2, CHARANUM
		;夢見る妖精モードなら堕落のお香について輝夜も見る必要があるはず、たぶん
		IF TALENT:COUNT:530 || TALENT:COUNT:553 || TALENT:COUNT:554
			IF GETBIT(CFLAG:COUNT:矯正アイテム, 0)
				kyosei += 1
			ENDIF
			IF GETBIT(CFLAG:COUNT:矯正アイテム, 1)
				kyosei:1 += 1
			ENDIF
		ENDIF
		;問題は数なのでこっちはこのまま
		IF GETBIT(FLAG:姉妹陥落フラグ, COUNT+1)
			kanraku += 1
		ENDIF
	NEXT
	;**ここまで**(その1)
	
	REPEAT 4
		IF LOCAL:1 != 503 && ITEM:(LOCAL:1)
			ITEMSALES:(LOCAL:1) = 0
		;矯正アイテムは装着できる姉妹がいなければならない
		ELSEIF LOCAL:1 == 504 && (kyosei == FLAG:攫った姉妹の数 || kanraku == FLAG:攫った姉妹の数)
			ITEMSALES:(LOCAL:1) = 0
		;矯正アイテムは装着できる姉妹がいなければならない
		ELSEIF LOCAL:1 == 505 && (kyosei:1 == FLAG:攫った姉妹の数 || kanraku == FLAG:攫った姉妹の数)
			ITEMSALES:(LOCAL:1) = 0
		ELSE
			ITEMSALES:(LOCAL:1) = 1
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
			BOUGHT = LOCAL:1
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL:1 += 1
	REND
ENDIF

;7:　娼婦用技能
IF ARG == 7
	LOCAL:1 = 380
	REPEAT 1
		IF TALENT:(LOCAL:1) && LOCAL:1 != 380
			ITEMSALES:(LOCAL:1) = 0
		;謎の魅力はLv3まである
		;Lv3にするには　$80000に加えて淫妖経験が800必要
		ELSEIF LOCAL:1 == 380 && TALENT:380 == 2
			IF EXP:淫妖経験 <= 800
				ITEMSALES:(LOCAL:1) = 0
				BOUGHT = LOCAL:0
				SETCOLOR 128, 128, 128
			ELSE
				ITEMSALES:(LOCAL:1) = 1
				BOUGHT = LOCAL:1
			ENDIF
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%　Lv3　-　淫妖経験が800必要
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		;謎の魅力はLv3まである
		;Lv3にするには　$80000に加えて淫妖経験が400必要
		ELSEIF LOCAL:1 == 380 && TALENT:380 == 1
			IF EXP:淫妖経験 <= 400
				ITEMSALES:(LOCAL:1) = 0
				BOUGHT = LOCAL:0
				SETCOLOR 128, 128, 128
			ELSE
				ITEMSALES:(LOCAL:1) = 1
				BOUGHT = LOCAL:1
			ENDIF
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%　Lv2　-　淫妖経験が400必要
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		;謎の魅力がLv3の場合、ここでストップをかける
		ELSEIF LOCAL:1 == 380 && TALENT:380 >= 3
			ITEMSALES:(LOCAL:1) = 0
		ELSE
			ITEMSALES:(LOCAL:1) = 1
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
			BOUGHT = LOCAL:1
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
		LOCAL:1 += 1
	REND
ENDIF

;8:　消耗品
IF ARG == 8
	LOCAL:1 = 533
	REPEAT 4
		;淡水色の聖水は一度しか販売されない
		IF FLAG:淡水色の聖水購入済み && LOCAL:1 == 536
			BREAK
		ENDIF
		ITEMSALES:(LOCAL:1) = 1
		IF LOCAL:1 == 536
			CALL SETCOLOR_PALAMUP
			PRINTFORM ☆[{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),18,LEFT%
		ELSE
			PRINTFORM [{LOCAL:1,3}] - %ITEMNAME:(LOCAL:1),20,LEFT%
		ENDIF
		BOUGHT = LOCAL:1
		CALL SETCOLOR_MONEY
		CALL ITEM_MONEY
		IF LOCAL:1 == 536
			PRINTFORML ${A,6}　一個限りの貴重品！
		ELSE
			PRINTFORML ${A,6}
		ENDIF
		RESETCOLOR
		LOCAL:1 += 1
	REND
	FOR L_LOCAL, 810, 818
		IF L_LOCAL != 813 && L_LOCAL != 814
			ITEMSALES:(L_LOCAL) = 1
			PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:(L_LOCAL),20,LEFT%
			BOUGHT = L_LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	NEXT
	
	L_LOCAL = 822
	
	ITEMSALES:(L_LOCAL) = 1
	PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:(L_LOCAL),20,LEFT%
	BOUGHT = L_LOCAL
	CALL SETCOLOR_MONEY
	CALL ITEM_MONEY
	PRINTFORML ${A,6}
	RESETCOLOR
	
	L_LOCAL = 824
	
	ITEMSALES:(L_LOCAL) = 1
	PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:(L_LOCAL),20,LEFT%
	BOUGHT = L_LOCAL
	CALL SETCOLOR_MONEY
	CALL ITEM_MONEY
	PRINTFORML ${A,6}
	RESETCOLOR
	
	;翡翠の指輪は主人への友好度が500を超えないと販売されない
	IF NO:TARGET == 1 && CFLAG:主人への友好度 >= 500
		L_LOCAL = 710
		
		ITEMSALES:(L_LOCAL) = 1
		PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:(L_LOCAL),20,LEFT%
		BOUGHT = L_LOCAL
		CALL SETCOLOR_MONEY
		CALL ITEM_MONEY
		PRINTFORML ${A,6}
		RESETCOLOR
	ENDIF
ENDIF

;10:　装備品
IF ARG == 10
	FOR L_LOCAL, 560, 601
		IF L_LOCAL <= 564 || (L_LOCAL >= 575 && L_LOCAL <= 579)
			IF ITEM:L_LOCAL
				ITEMSALES:L_LOCAL = 0
			ELSE
				ITEMSALES:L_LOCAL = 1
				PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:L_LOCAL,20,LEFT%
				BOUGHT = L_LOCAL
				CALL SETCOLOR_MONEY
				CALL ITEM_MONEY
				PRINTFORML ${A,6}
				RESETCOLOR
			ENDIF
			;弓と刀の境目に線を引く
			IF L_LOCAL == 564
				DRAWLINE
			ENDIF
		ENDIF
	NEXT
	DRAWLINE
	FOR L_LOCAL, 601, 603
		IF ITEM:L_LOCAL >= 100
			ITEMSALES:L_LOCAL = 0
		ELSE
			ITEMSALES:L_LOCAL = 1
			PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:L_LOCAL,20,LEFT%
			BOUGHT = L_LOCAL
			CALL SETCOLOR_MONEY
			CALL ITEM_MONEY
			PRINTFORML ${A,6}
			RESETCOLOR
		ENDIF
	NEXT
ENDIF

;11:行商アイテム
;・傾国のロープ
;・高脂肪ミルク
;以下ランダムに2種類
;・職業固有のアクセサリー
IF ARG == 11
	;傾国のロープ
	L_LOCAL = 68
	
	ITEMSALES:L_LOCAL = 1
	IF ITEM:L_LOCAL
		SETCOLOR 96,96,96
	ENDIF
	PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:L_LOCAL,20,LEFT%
	BOUGHT = L_LOCAL
	CALL SETCOLOR_MONEY
	CALL ITEM_MONEY
	PRINTFORML ${A,6}
	RESETCOLOR
	
	;高脂肪ミルク
	L_LOCAL = 539
	
	ITEMSALES:L_LOCAL = 1
	IF FLAG:行商ミルク購入済み
		SETCOLOR 96,96,96
	ENDIF
	PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:L_LOCAL,20,LEFT%
	BOUGHT = L_LOCAL
	CALL SETCOLOR_MONEY
	CALL ITEM_MONEY
	PRINTFORML ${A,6}
	RESETCOLOR
	
	
	
	FOR L_LOCAL, 0, 3
		;部位とアドレスをそれぞれランダムに選出
		LOCAL:2 = FLAG:(810+L_LOCAL)
		
		ITEMSALES:(LOCAL:2) = 1
		IF ITEM:(LOCAL:2)
			SETCOLOR 96,96,96
		ENDIF
		PRINTFORM [{LOCAL:2,3}] - %ITEMNAME:(LOCAL:2),20,LEFT%
		BOUGHT = (LOCAL:2)
		CALL SETCOLOR_MONEY
		CALL ITEM_MONEY
		PRINTFORML ${A,6}
		RESETCOLOR
	NEXT
	
ENDIF

;12:　娼婦用消耗品
;Ver0.997時点では娼婦のオーラのみ
IF ARG == 12
	;娼婦のオーラ
	L_LOCAL = 545
	
	ITEMSALES:L_LOCAL = 1
	PRINTFORM [{L_LOCAL,3}] - %ITEMNAME:L_LOCAL,20,LEFT%
	BOUGHT = L_LOCAL
	CALL SETCOLOR_MONEY
	CALL ITEM_MONEY
	PRINTFORML ${A,6}
	RESETCOLOR
ENDIF

PRINTL [999] - 戻る

$INPUT_LOOP
INPUT

IF RESULT == 999
	RETURN 0
ELSEIF ARG == 11 && RESULT == 539 && FLAG:行商ミルク購入済み
	PRINTL 売り切れです
	GOTO BACK_LOOP
ELSEIF RESULT == 68 && ITEM:RESULT
	PRINTL そのアイテムは既に持っています
	GOTO BACK_LOOP
ELSEIF INRANGE(RESULT, 400, 450) && ITEM:RESULT
	PRINTL そのアイテムは既に持っています
	GOTO BACK_LOOP
ELSEIF INRANGE(RESULT , 0 , VARSIZE("ITEM") - 1) && ITEMSALES:RESULT == 1
	BOUGHT = RESULT
	CALL ITEM_MONEY
	;呪詛の布切れ以外は値段を5倍にする
	IF INRANGE( BOUGHT, 10, 19 ) && ITEMNAME:BOUGHT != "呪詛の布切れ"
		A *= 5
	ENDIF
	DRAWLINE
	PRINTFORML ○%ITEMNAME:BOUGHT%
	PRINT 　
	CALLFORM ITEM_DETAIL_{BOUGHT}
	IF ARG == 1 || ARG == 2 || BOUGHT == 68
		LOCAL = CFLAG:服装
		CALL DATA_CHANGE_CLOTHE, BOUGHT, "表示変更"
		CALL DATA_CHANGE_CLOTHE, LOCAL, "変更"
	ENDIF
	DRAWLINE
	
	;最悪の目覚めとジェルクリーナー、退魔の松明のみ複数購入可
	IF BOUGHT == 503 || BOUGHT == 601 || BOUGHT == 602
		IF MONEY < A
			PRINTL 所持金が足りないようですね……
			GOTO BACK_LOOP
		ENDIF
		
		PRINTL いくつ買いますか？(0でキャンセル)
		PRINTL [1] - 1個
		IF MONEY >= A*2
			PRINTL [2] - 2個
		ENDIF
		IF MONEY >= A*5
			PRINTL [5] - 5個
		ENDIF
		PRINTFORML [{MONEY/A}] - 買えるだけ
		PRINTFORML [0] - キャンセル

		$INPUT_LOOP_503
		INPUT
		
		;0が入力された場合、キャンセルする
		IF RESULT == 0
			GOTO BACK_LOOP
		ELSEIF MONEY >= A*RESULT
			A *= RESULT
			kosuu = RESULT
		ELSE
			PRINTL 所持金が足りませんね……
			GOTO INPUT_LOOP_503
		ENDIF
	ENDIF
	
	IF BOUGHT >= 490 && BOUGHT <= 500
		PRINTFORML %ITEMNAME:BOUGHT%は${A}となります。購入しますか？
		PRINTFORML （10ターン使用すると無くなります）
	ELSE
		PRINTFORML %ITEMNAME:BOUGHT%は${A}となります。購入しますか？
	ENDIF
	PRINTL [0] - はい
	PRINTL [1] - いいえ
	
	CALL SENTAKUSHI, 2
	
	IF RESULT == 0
		IF MONEY < A
			PRINTL 所持金が足りないようですね……
			GOTO BACK_LOOP
		;**ここからPetitPatch4**（その2）
		;現在手元にいる姉妹が全員陥落している場合、矯正アイテムを購入できない
		ELSEIF BOUGHT == 504 || BOUGHT == 505
			;変数の初期化
			VARSET check, 0
			FOR COUNT, 2, CHARANUM
				;該当するアイテムを装着していない場合、表示
				;(陥落していないことも条件に入る)
				;姉妹陥落フラグじゃなくてこっちを使うことに
				;IF !GETBIT(CFLAG:COUNT:矯正アイテム, BOUGHT-504) && !GETBIT(FLAG:姉妹陥落フラグ, COUNT-1)
				IF (TALENT:COUNT:530 || TALENT:COUNT:553 || TALENT:COUNT:554) && !GETBIT(CFLAG:COUNT:矯正アイテム, BOUGHT-504) && !CFLAG:COUNT:対象選択可能
					LOCAL:COUNT = 1
					check += 1
				ELSE
					LOCAL:COUNT = 0
				ENDIF
			NEXT
			;手元に居る姉妹が全員陥落している
			IF !check
				PRINTL 装着する対象がいませんね……
				GOTO BACK_LOOP
			ENDIF
		ENDIF
		MONEY -= A
		
		;矯正アイテムの場合、装着対象を選ぶ
		IF BOUGHT == 504 || BOUGHT == 505
			PRINTL 誰に装着しますか？
			FOR COUNT, 2, CHARANUM
				;該当するアイテムを装着していない場合、表示
				;(陥落していないことも条件に入る)
				IF LOCAL:COUNT
					PRINTFORML [{COUNT}] - %CALLNAME:COUNT%
				ENDIF
			NEXT
			
			$INPUT_LOOP_S
			INPUT
			
			IF LOCAL:RESULT && RESULT <= CHARANUM-1
				PRINTFORML %CALLNAME:RESULT%に%ITEMNAME:BOUGHT%を装着した
				IF BOUGHT == 504
					CFLAG:RESULT:矯正アイテム |= 1p0
				ELSEIF BOUGHT == 505
					CFLAG:RESULT:矯正アイテム |= 1p1
				ENDIF
			ELSE
				PRINTL 入力が不正です
				GOTO INPUT_LOOP_S
			ENDIF
		;**ここまで**
		
		;技能の場合
		ELSEIF ARG == 5 || ARG == 7
			;謎の魅力はレベルアップ制
			IF BOUGHT == 380
				TALENT:BOUGHT += 1
				PRINTFORML %TALENTNAME:BOUGHT%　Lv{TALENT:BOUGHT}を習得した
			ELSE
				TALENT:BOUGHT = 1
				PRINTFORML %TALENTNAME:BOUGHT%を習得した
			ENDIF
		;装備品の場合
		ELSEIF ARG == 10 && BOUGHT <= 600
			PRINTFORML ここで装備しますか？
			PRINTL [0] - はい
			PRINTL [1] - いいえ
			
			CALL SENTAKUSHI, 2
			
			IF RESULT == 0
				PRINTFORML %CALLNAME:TARGET%は%ITEMNAME:BOUGHT%を装備した
				CFLAG:装備 = BOUGHT
			ENDIF
			ITEM:BOUGHT += 1
		;入浴剤の場合
		ELSEIF BOUGHT >= 490 && BOUGHT <= 499
			ITEM:BOUGHT = 10
			PRINTFORML %ITEMNAME:BOUGHT%を購入した
			
			;装備条件を満たしている場合、その場で変更できるようにする
			IF ABLE_ACC(BOUGHT)
				PRINTFORML 早速[入浴剤]として設定しますか？
				PRINTL [0] - はい
				PRINTL [1] - いいえ
				
				CALL SENTAKUSHI, 2
				
				IF !RESULT
					PRINTFORML %ITEMNAME:BOUGHT%を入浴剤として設定した！
					CFLAG:日用品／入浴剤 = BOUGHT
				ENDIF
			ENDIF
		ELSE
			IF BOUGHT == 503 || (BOUGHT >= 601 && ARG == 10)
				ITEM:BOUGHT += kosuu
			;消耗品の場合は増やす
			ELSEIF ARG == 8
				ITEM:BOUGHT += 1
			;高脂肪ミルクの場合も増やす
			ELSEIF BOUGHT == 539
				ITEM:BOUGHT += 1
			ELSE
				ITEM:BOUGHT = 1
			ENDIF
			PRINTFORML %ITEMNAME:BOUGHT%を購入した
			
			;行商ミルク
			IF ARG == 11 && BOUGHT == 539
				FLAG:行商ミルク購入済み = 1
			ENDIF
			;淡水色の聖水
			IF ARG == 8 && BOUGHT == 536
				FLAG:淡水色の聖水購入済み = 1
			ENDIF
		ENDIF
		GOTO BACK_LOOP
	ELSE
		GOTO BACK_LOOP
	ENDIF
ELSE
	PRINTL 売っていません
	GOTO INPUT_LOOP
ENDIF

