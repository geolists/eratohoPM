;========================================================
;シスタールート
;
;分岐チャート
;1:最も高い能力を見る
;2:その能力の数値で分岐

;========================================================
@ENDING_SISTER

;LOCAL:1に見たエンディングのIDを格納するため、一旦初期化する
LOCAL:1= 0

;堕落+淫妖が900以上
IF BASE:堕落 + BASE:淫妖 >= 900
	CALL ENDING_SISTER_1
	LOCAL:1 = 740
;600以上
ELSEIF BASE:堕落 + BASE:淫妖 >= 600
	CALL ENDING_SISTER_2
	LOCAL:1 = 741
;全能力が900以上
ELSEIF CHECK_LIMITBASE(900, "美麗", "体力") >= 6
	CALL ENDING_SISTER_18
	LOCAL:1 = 757
;全能力が600以上
ELSEIF CHECK_LIMITBASE(600, "美麗", "体力") >= 6
	CALL ENDING_SISTER_19
	LOCAL:1 = 758
;全能力が300以上
ELSEIF CHECK_LIMITBASE(300, "美麗", "体力") >= 6
	CALL ENDING_SISTER_20
	LOCAL:1 = 759
ELSE
	;各能力の内高い能力をサーチ
	LOCAL = MAXARRAY(BASE, 5, 12)
	FINDELEMENT BASE, LOCAL, 5, 12

	LOCAL:1 = RESULT
	;最も高い能力が美麗
	IF LOCAL:1 == 5
		;900以上
		IF LOCAL >= 900
			CALL ENDING_SISTER_3
			LOCAL:1 = 742
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_SISTER_4
			LOCAL:1 = 743
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_SISTER_5
			LOCAL:1 = 744
		ENDIF
	;知力
	ELSEIF LOCAL:1 == 6
		;900以上
		IF LOCAL >= 900
			CALL ENDING_SISTER_6
			LOCAL:1 = 745
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_SISTER_7
			LOCAL:1 = 746
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_SISTER_8
			LOCAL:1 = 747
		ENDIF
	;疎通
	ELSEIF LOCAL:1 == 7
		;900以上
		IF LOCAL >= 900
			CALL ENDING_SISTER_9
			LOCAL:1 = 748
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_SISTER_10
			LOCAL:1 = 749
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_SISTER_11
			LOCAL:1 = 750
		ENDIF
	;家事
	ELSEIF LOCAL:1 == 8
		;900以上
		IF LOCAL >= 900
			CALL ENDING_SISTER_12
			LOCAL:1 = 751
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_SISTER_13
			LOCAL:1 = 752
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_SISTER_14
			LOCAL:1 = 753
		ENDIF
	;信仰
	ELSEIF LOCAL:1 == 9
		;900以上
		IF LOCAL >= 900
			CALL ENDING_SISTER_15
			LOCAL:1 = 754
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_SISTER_16
			LOCAL:1 = 755
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_SISTER_17
			LOCAL:1 = 756
		ENDIF
	ENDIF
ENDIF

RETURN LOCAL:1

