;　　　　　　　　　　　　　+1未満　→　ED2へ

;========================================================
@ENDING_SYOUHU



;王宮を追放されている
IF FLAG:王宮追放
	;10回以上攫われ、その内助けを頼んだ回数が5回未満
	IF FLAG:誘拐回数 >= 10 && FLAG:助けを頼んだ回数 < 5
		CALL ENDING_SYOUHU_3
	;全能力が600以上
	ELSEIF CHECK_LIMITBASE(600, "美麗", "体力") >= 6
			CALL ENDING_SYOUHU_9
	;全能力が300以上
	ELSEIF CHECK_LIMITBASE(600, "美麗", "体力") >= 6
		CALL ENDING_SYOUHU_10
	ELSE
		;メイドが50％
		IF CFLAG:属性／メイド == 50
			CALL ENDING_SYOUHU_5
		;シスターが50％
		ELSEIF CFLAG:属性／シスター == 50
			CALL ENDING_SYOUHU_6
		;ゴスロリが50％
		ELSEIF CFLAG:属性／ゴスロリ == 50
			CALL ENDING_SYOUHU_7
		;花嫁が50％
		ELSEIF CFLAG:属性／花嫁 == 50
			CALL ENDING_SYOUHU_8
		ELSE
			CALL ENDING_SYOUHU_4
		ENDIF
	ENDIF
ELSE
	;アライメントが+0以上
	IF BASE:アライメント > 100
		CALL ENDING_SYOUHU_1
	ELSE
		CALL ENDING_SYOUHU_2
	ENDIF
ENDIF

