;========================================================
;姫様ルート
;
;分岐チャート
;1:最も高い能力を見る
;2:その能力の数値で分岐

;========================================================
@ENDING_HIME

;LOCAL:1に見たエンディングのIDを格納するため、一旦初期化する
LOCAL:1= 0

;堕落が900以上
IF BASE:堕落 >= 900
	CALL ENDING_HIME_1
	LOCAL:1 = 700
;600以上
ELSEIF BASE:堕落 >= 600
	CALL ENDING_HIME_2
	LOCAL:1 = 701
;全能力が900以上
ELSEIF CHECK_LIMITBASE(900, "美麗", "体力") >= 6
	;全能力が900以上＆淫妖と堕落経験が0
	IF !EXP:淫妖経験 && !EXP:堕落経験
		CALL ENDING_HIME_21
		;前世譚プレイ中の場合、エンディングフラグを立てない
		IF !FLAG:前世譚プレイ
			LOCAL:1 = 883
		ENDIF
	ELSE
		CALL ENDING_HIME_18
		LOCAL:1 = 717
	ENDIF
;全能力が600以上
ELSEIF CHECK_LIMITBASE(600, "美麗", "体力") >= 6
	CALL ENDING_HIME_19
	LOCAL:1 = 718
;全能力が300以上
ELSEIF CHECK_LIMITBASE(300, "美麗", "体力") >= 6
	CALL ENDING_HIME_20
	LOCAL:1 = 719
ELSE
	;各能力の内高い能力をサーチ
	LOCAL = MAXARRAY(BASE, 5, 12)
	FINDELEMENT BASE, LOCAL, 5, 12

	LOCAL:1 = RESULT
	;最も高い能力が美麗
	IF LOCAL:1 == 5
		;900以上
		IF LOCAL >= 900
			CALL ENDING_HIME_3
			LOCAL:1 = 702
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_HIME_4
			LOCAL:1 = 703
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_HIME_5
			LOCAL:1 = 704
		ENDIF
	;知力
	ELSEIF LOCAL:1 == 6
		;900以上
		IF LOCAL >= 900
			CALL ENDING_HIME_6
			LOCAL:1 = 705
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_HIME_7
			LOCAL:1 = 706
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_HIME_8
			LOCAL:1 = 707
		ENDIF
	;疎通
	ELSEIF LOCAL:1 == 7
		;900以上
		IF LOCAL >= 900
			CALL ENDING_HIME_9
			LOCAL:1 = 708
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_HIME_10
			LOCAL:1 = 709
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_HIME_11
			LOCAL:1 = 710
		ENDIF
	;家事
	ELSEIF LOCAL:1 == 8
		;900以上
		IF LOCAL >= 900
			CALL ENDING_HIME_12
			LOCAL:1 = 711
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_HIME_13
			LOCAL:1 = 712
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_HIME_14
			LOCAL:1 = 713
		ENDIF
	;信仰
	ELSEIF LOCAL:1 == 9
		;900以上
		IF LOCAL >= 900
			CALL ENDING_HIME_15
			LOCAL:1 = 714
		;700以上
		ELSEIF LOCAL >= 700
			CALL ENDING_HIME_16
			LOCAL:1 = 715
		;400以上
		ELSEIF LOCAL >= 300
			CALL ENDING_HIME_17
			LOCAL:1 = 716
		ENDIF
	ENDIF
ENDIF

RETURN LOCAL:1

