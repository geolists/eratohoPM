;=====================================================================
;一言メッセージ
;メニュー画面で表示されます
;ARG　　メッセージ固定用の引数
;ARG:1　キャラ指定
;=====================================================================
@HIMESSAGE, ARG, ARG:1
#DIM YOUSO, 1

;衣服の属性
#DIM CLO_ELE, 1

;どの純化口上を呼び出すかを保存する変数
#DIM JUNKA, 1
;初期化もしておく(#DIM JUNKA=0でできた気がしなくもない)
JUNKA = 0

;LOCAL:1の初期化
LOCAL:1 = 0

VARSET CFLAG, 0, 190, 195

CALL CLOTHE_ABL, "属性"
CLO_ELE = RESULT


;他の魂が入っている(魂抜き時)
IF CFLAG:魂の状態 == 1
	FOR COUNT, 0, 5
		LOCAL = MINARRAY(CFLAG, 190, 195)
		FINDELEMENT CFLAG, LOCAL, 190, 195
		YOUSO = RESULT
		IF LOCAL == 0
			;今の服装が布切れ
			IF !ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)
				CFLAG:YOUSO = 1
				LOCAL:1 += 1
			;性格が淫乱
;			ELSEIF CFLAG:性格 == -1 && !CHECKMES(3)
;				CFLAG:YOUSO = 3
;				LOCAL:1 += 1
			;思考力が100未満で躾方針が快楽を与える
			ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100 && !CHECKMES(10)
				CFLAG:YOUSO = 10
				LOCAL:1 += 1
			;妊娠している
			ELSEIF TALENT:妊娠 && !CHECKMES(11)
				CFLAG:YOUSO = 11
				LOCAL:1 += 1
			;淫魔になっている
			ELSEIF TALENT:淫魔 && !CHECKMES(12)
				CFLAG:YOUSO = 12
				LOCAL:1 += 1
			ELSEIF !LOCAL:1 && !CHECKMES(99)
				CFLAG:YOUSO = 99
				LOCAL:1 += 1
			ENDIF
		ENDIF
	NEXT
	IF LOCAL:1 != 1
		CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
	ELSE
		CFLAG:決定要素 = CFLAG:190
	ENDIF
	CALLFORM HIMESSAGE_SOUL_{CFLAG:追加魂}_{CFLAG:決定要素}
;他の魂が入っている(依頼時)
ELSEIF CFLAG:魂の状態 == 3
	;魂の種類によって分ける

	TRYCALLFORM HIMESSAGE_SOUL_{CFLAG:追加魂}, MIN(CFLAG:淫妖蓄積値/70, 4)
;[純化]を得ている場合
ELSEIF TALENT:純化
	
	;どの純化属性を得ているかを取得する
	;TALENT20～27をサーチして、ひっかかるのがあればそれを代入する
	FOR COUNT, 20, 28
		IF TALENT:COUNT
			;純化要素を変数に格納する
			JUNKA = COUNT-20
			;このFOR文は用済みなので、BREAK文で抜ける
			BREAK
		ENDIF
	NEXT
	
	IF !ARG
		FOR COUNT, 0, 5
			LOCAL = MINARRAY(CFLAG, 190, 195)
			FINDELEMENT CFLAG, LOCAL, 190, 195
			YOUSO = RESULT
			IF LOCAL == 0
				;今の服装が布切れor半裸以上の服を着ている
				IF (!ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)) || (CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):11 && !CHECKMES(13))
					CFLAG:YOUSO = 1
					LOCAL:1 += 1
				;1年目1月の頭
				ELSEIF FLAG:年数 == 1 && DAY == 1 && TIME == 0 && !CHECKMES(2)
					CFLAG:YOUSO = 2
					LOCAL:1 += 1
				;性格が淫乱
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(3)
					CFLAG:YOUSO = 3
					LOCAL:1 += 1
				;性格が淫乱(その2)
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(6)
					CFLAG:YOUSO = 6
					LOCAL:1 += 1
				;やる気が2以下の場合
				ELSEIF CFLAG:やる気 <= 2 && !CHECKMES(9)
					CFLAG:YOUSO = 9
					LOCAL:1 += 1
				;思考力が100未満で躾方針が快楽を与える
				ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100 && !CHECKMES(10)
					CFLAG:YOUSO = 10
					LOCAL:1 += 1
				;妊娠している
				ELSEIF TALENT:妊娠 && !CHECKMES(11)
					CFLAG:YOUSO = 11
					LOCAL:1 += 1
				;淫魔になっている
				ELSEIF TALENT:淫魔 && !CHECKMES(12)
					CFLAG:YOUSO = 12
					LOCAL:1 += 1
				ELSEIF !LOCAL:1 && !CHECKMES(99)
					CFLAG:YOUSO = 99
					LOCAL:1 += 1
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 != 1
			CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
		ELSE
			CFLAG:決定要素 = CFLAG:190
		ENDIF
		CALLFORM HIMESSAGE_JUNKA_{LOCAL}_{CFLAG:決定要素}
	ENDIF
;固定されていない場合
ELSEIF TALENT:輝夜
	;夜遊びの球使用中は固定の口上を出す
	IF CFLAG:使用中／夜遊びの球
		CFLAG:決定要素 = 13
	ELSEIF !ARG
		FOR COUNT, 0, 5
			LOCAL = MINARRAY(CFLAG, 190, 195)
			FINDELEMENT CFLAG, LOCAL, 190, 195
			YOUSO = RESULT
			IF LOCAL == 0
				;今の服装が布切れor半裸以上の服を着ている
				IF (!ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)) || (CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):11 && !CHECKMES(1))
					CFLAG:YOUSO = 1
					LOCAL:1 += 1
				;1年目1月の頭
				ELSEIF FLAG:年数 == 1 && DAY == 1 && TIME == 0 && !CHECKMES(2)
					CFLAG:YOUSO = 2
					LOCAL:1 += 1
				;性格が淫乱
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(3)
					CFLAG:YOUSO = 3
					LOCAL:1 += 1
				;メイド属性の服を着ている(その1)
				ELSEIF GETBIT(CLO_ELE,2) && !TALENT:娼婦 && !CHECKMES(4)
					CFLAG:YOUSO = 4
					LOCAL:1 += 1
				;シスター属性の服を着ている(その1)
				ELSEIF GETBIT(CLO_ELE,3) && !TALENT:娼婦 && !CHECKMES(5)
					CFLAG:YOUSO = 5
					LOCAL:1 += 1
				;性格が淫乱(その2)
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(6)
					CFLAG:YOUSO = 6
					LOCAL:1 += 1
				;やる気が2以下の場合
				ELSEIF CFLAG:やる気 <= 2 && !CHECKMES(9)
					CFLAG:YOUSO = 9
					LOCAL:1 += 1
				;思考力が100未満で躾方針が快楽を与える
				ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100 && !CHECKMES(10)
					CFLAG:YOUSO = 10
					LOCAL:1 += 1
				;妊娠している
				ELSEIF TALENT:妊娠 && !CHECKMES(11)
					CFLAG:YOUSO = 11
					LOCAL:1 += 1
				;淫魔になっている
				ELSEIF TALENT:淫魔 && !CHECKMES(12)
					CFLAG:YOUSO = 12
					LOCAL:1 += 1
				ELSEIF !LOCAL:1 && !CHECKMES(99)
					CFLAG:YOUSO = 99
					LOCAL:1 += 1
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 != 1
			CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
		ELSE
			CFLAG:決定要素 = CFLAG:190
		ENDIF
	ENDIF
;依姫
ELSEIF TALENT:依姫
	IF !ARG
		FOR COUNT, 0, 5
			LOCAL = MINARRAY(CFLAG, 190, 195)
			FINDELEMENT CFLAG, LOCAL, 190, 195
			YOUSO = RESULT
			IF LOCAL == 0
				;普段着を売り、今の服が布切れ
			;	IF !ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)
			;		CFLAG:YOUSO = 1
			;		LOCAL:1 += 1
				;性格が淫乱
				IF CFLAG:性格 == -1 && !CHECKMES(3)
					CFLAG:YOUSO = 3
					LOCAL:1 += 1
				;メイド属性の服を着ている(その1)
			;	ELSEIF GETBIT(CLO_ELE,2) && !TALENT:娼婦 && !CHECKMES(4)
			;		CFLAG:YOUSO = 4
			;		LOCAL:1 += 1
			;	;シスター属性の服を着ている(その1)
			;	ELSEIF GETBIT(CLO_ELE,3) && !TALENT:娼婦 && !CHECKMES(5)
			;		CFLAG:YOUSO = 5
			;		LOCAL:1 += 1
				;性格が淫乱(その2)
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(6)
					CFLAG:YOUSO = 6
					LOCAL:1 += 1
				;思考力が200未満
				ELSEIF BASE:思考力 < 200 && !CHECKMES(7)
					CFLAG:YOUSO = 7
					LOCAL:1 += 1
				;常識が200未満
				ELSEIF BASE:常識 < 200 && !CHECKMES(8)
					CFLAG:YOUSO = 8
					LOCAL:1 += 1
				;思考力が100未満で躾方針が快楽を与える
				ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100
					CFLAG:YOUSO = 10
					LOCAL:1 += 1
				ELSEIF !LOCAL:1 && !CHECKMES(99)
					CFLAG:YOUSO = 99
					LOCAL:1 += 1
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 != 1
			CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
		ELSE
			CFLAG:決定要素 = CFLAG:190
		ENDIF
	ENDIF
;豊姫
ELSEIF TALENT:豊姫
	IF !ARG
		FOR COUNT, 0, 5
			LOCAL = MINARRAY(CFLAG, 190, 195)
			FINDELEMENT CFLAG, LOCAL, 190, 195
			YOUSO = RESULT
			IF LOCAL == 0
				;普段着を売り、今の服が布切れ
			;	IF !ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)
			;		CFLAG:YOUSO = 1
			;		LOCAL:1 += 1
				;性格が淫乱
				IF CFLAG:性格 == -1 && !CHECKMES(3)
					CFLAG:YOUSO = 3
					LOCAL:1 += 1
				;メイド属性の服を着ている(その1)
			;	ELSEIF GETBIT(CLO_ELE,2) && !TALENT:娼婦 && !CHECKMES(4)
			;		CFLAG:YOUSO = 4
			;		LOCAL:1 += 1
			;	;シスター属性の服を着ている(その1)
			;	ELSEIF GETBIT(CLO_ELE,3) && !TALENT:娼婦 && !CHECKMES(5)
			;		CFLAG:YOUSO = 5
			;		LOCAL:1 += 1
				;性格が淫乱(その2)
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(6)
					CFLAG:YOUSO = 6
					LOCAL:1 += 1
				;思考力が200未満
				ELSEIF BASE:思考力 < 200 && !CHECKMES(7)
					CFLAG:YOUSO = 7
					LOCAL:1 += 1
				;常識が200未満
				ELSEIF BASE:常識 < 200 && !CHECKMES(8)
					CFLAG:YOUSO = 8
					LOCAL:1 += 1
				;思考力が100未満で躾方針が快楽を与える
				ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100
					CFLAG:YOUSO = 10
					LOCAL:1 += 1
				ELSEIF !LOCAL:1 && !CHECKMES(99)
					CFLAG:YOUSO = 99
					LOCAL:1 += 1
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 != 1
			CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
		ELSE
			CFLAG:決定要素 = CFLAG:190
		ENDIF
	ENDIF
;その他
ELSE
	ARG:1 = 999
	IF !ARG
		FOR COUNT, 0, 5
			LOCAL = MINARRAY(CFLAG, 190, 195)
			FINDELEMENT CFLAG, LOCAL, 190, 195
			YOUSO = RESULT
			IF LOCAL == 0
				;普段着を売り、今の服が布切れ
				IF !ITEM:0 && CFLAG:服装 < 0 && !CHECKMES(1)
					CFLAG:YOUSO = 1
					LOCAL:1 += 1
				;性格が淫乱
				ELSEIF CFLAG:性格 == -1 && !CHECKMES(3)
					CFLAG:YOUSO = 3
					LOCAL:1 += 1
				;メイド属性の服を着ている(その1)
				ELSEIF GETBIT(CLO_ELE,2) && !TALENT:娼婦 && !CHECKMES(4)
					CFLAG:YOUSO = 4
					LOCAL:1 += 1
				;シスター属性の服を着ている(その1)
				ELSEIF GETBIT(CLO_ELE,3) && !TALENT:娼婦 && !CHECKMES(5)
					CFLAG:YOUSO = 5
					LOCAL:1 += 1
			;	;思考力が200未満
			;	ELSEIF BASE:思考力 < 200 && !CHECKMES(7)
			;		CFLAG:YOUSO = 7
			;		LOCAL:1 += 1
			;	;常識が200未満
			;	ELSEIF BASE:常識 < 200 && !CHECKMES(8)
			;		CFLAG:YOUSO = 8
			;		LOCAL:1 += 1
				;思考力が100未満で躾方針が快楽を与える
				ELSEIF BASE:思考力 < 100 && CFLAG:躾方針 == 2 && BASE:感度 >= 100
					CFLAG:YOUSO = 10
					LOCAL:1 += 1
				ELSEIF !LOCAL:1 && !CHECKMES(99)
					CFLAG:YOUSO = 99
					LOCAL:1 += 1
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 != 1
			CFLAG:決定要素 = CFLAG:(190 + RAND:(LOCAL:1))
		ELSE
			CFLAG:決定要素 = CFLAG:190
		ENDIF
	ENDIF
ENDIF

