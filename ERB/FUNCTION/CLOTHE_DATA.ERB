
;===================================
;衣装ごとのステータス補正
;===================================
@CLOTHE_ABL, ARGS

LOCAL = 0
;布切れ系統はIDがマイナスのため、関数名が独立している
IF CFLAG:服装 < 0
	CALLFORM CLOTHE_CLOTH_{CFLAG:服装*-1}, ARGS
ELSE
	CALLFORM CLOTHE_ID_{CFLAG:服装}, ARGS
ENDIF
LOCAL = RESULT
CALL REMODEL_HOSEI, ARGS
LOCAL += RESULT
CALL CLOTHE_EFFECT_AFTER, ARGS

RETURN LOCAL


