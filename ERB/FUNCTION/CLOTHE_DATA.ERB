;CALL CLOTHE_ABL, "属性"

IF CFLAG:服装 >= 0
; && !GETBIT(RESULT, 6) && !GETBIT(RESULT, 7)
	VARSET CFLAG, 0, 310, 325
	;スカート丈が違い、変更不可でない服装の場合、追加で補正をかける
	IF DITEMTYPE:(CFLAG:服装):0 && !DITEMTYPE:(CFLAG:服装):10
		美麗 += 1 * DITEMTYPE:(CFLAG:服装):0
		淫妖 += 1 * DITEMTYPE:(CFLAG:服装):0
	ENDIF
	
	;絹の布の場合、美麗+2　＆　淫妖+1
	SELECTCASE DITEMTYPE:(CFLAG:服装):2
		CASE 1
			美麗 += 2
			淫妖 += 1
	ENDSELECT
	
	;適正バストサイズ以内にバストが収まっている場合、美麗補正+1
	CALL CLOTHE_BUSTSIZE, CFLAG:服装
	LOCAL = RESULT
	
	IF BASE:胸囲 <= LOCAL
		美麗 += 1
	ENDIF
ENDIF

