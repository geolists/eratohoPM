	CFLAG:盗賊ムード += 20
ELSEIF BASE:淫妖 >= 300
	CFLAG:盗賊ムード += 15
ELSE
	CFLAG:盗賊ムード += 10
ENDIF

;服が布切れか全裸の場合　ムードが4増加する
IF CFLAG:服装 < 0 || (CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):11 == 2)
	CFLAG:盗賊ムード += 4
;半裸の場合は+2
ELSEIF CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):11 == 1
	CFLAG:盗賊ムード += 2
ENDIF

;全壊の場合　+3
IF CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):14 == 2
	CFLAG:盗賊ムード += 3
;半壊の場合　+2
ELSEIF CFLAG:服装 >= 0 && DITEMTYPE:(CFLAG:服装):14 == 1
	CFLAG:盗賊ムード += 2
ENDIF

;性格が高飛車の場合、ムード+3
IF CFLAG:性格 == 2
	CFLAG:盗賊ムード += 3
ENDIF

;媚薬状態の場合、+2
IF CFLAG:媚薬状態 == 1
	CFLAG:盗賊ムード += 2
ENDIF

;小麦色なら+4
IF CFLAG:肌の色 == 2
	CFLAG:盗賊ムード += 4
;薄茶色なら+2
ELSEIF CFLAG:肌の色 == 1
	CFLAG:盗賊ムード += 2
ENDIF

;動物耳がある場合、+3
IF TALENT:動物耳
	CFLAG:盗賊ムード += 3
ENDIF

;理性を10減らす(補正はかかる)
CALL CALCULATION_PALAM, 10, "理性"

;行動回数が0になってるなら終了
IF BASE:行動回数 <= 0
	;理性が0だった場合、回復する
	IF !BASE:理性
		CALL SETCOLOR_PALAMUP
		PRINTFORML %CALLNAME:TARGET%の瞳に光が戻った！
		BASE:理性 = MAXBASE:理性/2
		RESETCOLOR
	ENDIF
	
	FLAG:見せしめ実行 = 0
	DRAWLINE
	CALL CHANGE_ELEMENT, CFLAG:属性強度
	
	IF CFLAG:媚薬状態 == 1
		CFLAG:媚薬状態 = 0
	ENDIF
	
	;淫魔なら経験を増やす
	IF TALENT:淫魔
		EXP:淫魔経験 += 1
	ENDIF
	CALL CHECK_SUC_SKILL
	
	;媚薬を飲んでいない場合、情報を聞き出せる
	IF !CFLAG:媚薬状態 && FLAG:王宮追放
		FLAG:王宮情報 += 20 + 30 * TALENT:謎の魅力
	ENDIF
	RETURN 0
ELSE
	GOTO LOOP
ENDIF

RETURN 0

