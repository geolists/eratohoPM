	;相手の衣服が半裸以上の場合
	IF TFLAG:609 && !RAND:4
		;愛撫を行う
		TFLAG:(536+ARG) = 102
	ELSE
		;相手の衣服が全裸でない場合、服を剥ぐ
		IF TFLAG:609 != 2 && !(TFLAG:ターン数 % 3)
			TFLAG:(536+ARG) = 200
		ELSE
			;威力80の通常攻撃
			TFLAG:(536+ARG) = 100
		ENDIF
	ENDIF
;2:　匪賊
ELSEIF LOCAL == 2
	;相手の衣服が半裸以上の場合
	IF TFLAG:609 && !RAND:4
		;愛撫を行う
		TFLAG:(536+ARG) = 102
	ELSE
		IF !(TFLAG:ターン数 % 3)
			TFLAG:(536+ARG) = 100
		ELSE
			;威力80の通常攻撃
			TFLAG:(536+ARG) = 100
		ENDIF
	ENDIF
;3:　下級匪賊
ELSEIF LOCAL == 3
	IF !(TFLAG:ターン数 % 3)
		IF FLAG:特殊戦闘
			;特殊戦闘用のＡＩを呼び出す
			CALL AI_SPE, ARG
		ELSE
			TFLAG:(536+ARG) = 100
		ENDIF
	ELSE
		;威力80の通常攻撃
		TFLAG:(536+ARG) = 100
	ENDIF
;4:　かじりネズミ
ELSEIF LOCAL == 4
	IF !(TFLAG:ターン数 % 3)
		TFLAG:(536+ARG) = 100
	ELSE
		;威力80の通常攻撃
		TFLAG:(536+ARG) = 100
	ENDIF
;5:　狼
ELSEIF LOCAL == 5
	IF !(TFLAG:ターン数 % 3)
		TFLAG:(536+ARG) = 101
	ELSE
		;威力80の通常攻撃
		TFLAG:(536+ARG) = 100
	ENDIF
;6:　スライム
ELSEIF LOCAL == 6
	;相手の気力が70％以下
	IF TFLAG:(557)*10 <= TFLAG:(551)*7
		IF !(TFLAG:ターン数 % 3)
			;淫靡な香り
