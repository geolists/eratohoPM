	;助手が射精フラグ
	TFLAG:6 = 2
;通常の射精
ELSEIF E == 1
	;助手が射精フラグ
	TFLAG:6 = 1
ENDIF

@SAMEN_SHOOT_CALC
;射精時の共通処理
;精液汚れの設定
STAIN:P:2 |= 4

;大量射精
IF E == 2
	EXP:P:3 += 2
	PRINTL 大量射精

	BASE:P:2 -= EJAC*2
	SIF BASE:P:2 >= EJAC
		BASE:P:2 = EJAC-1
;通常の射精
ELSEIF E == 1
	EXP:P:3 += 1
	PRINTL 射精

	BASE:P:2 -= EJAC
	SIF BASE:P:2 >= EJAC
		BASE:P:2 = EJAC-1
ENDIF

;射精時に中毒充足のソースを追加
;通常射精時
SIF E == 1
	A = 200
;大量射精時
SIF E == 2
	A = 500

IF E > 0
	;精液中毒を見る
	IF ABL:16 == 1
		TIMES A , 0.00
	ELSEIF ABL:16 == 2
		TIMES A , 0.30
	ELSEIF ABL:16 == 3
		TIMES A , 0.60
	ELSEIF ABL:16 == 4
		TIMES A , 0.90
	ELSEIF ABL:16 == 5
		TIMES A , 1.20
	ELSEIF ABL:16 == 6
		TIMES A , 1.50
	ELSEIF ABL:16 == 7
		TIMES A , 1.80
	ELSEIF ABL:16 == 8
		TIMES A , 2.30
	ELSEIF ABL:16 == 9
		TIMES A , 3.00
	ELSEIF ABL:16 == 10
		TIMES A , 5.00
	ENDIF
ENDIF

SOURCE:9 += A

;射精後は再装填の時間を設ける
IF SELECTCOM == 124 || SELECTCOM == 125
	;通常射精時
	SIF E == 1
		CFLAG:MASTER:24 = 3
	;大量射精時
	SIF E == 2
		CFLAG:MASTER:24 = 5
	CFLAG:MASTER:11 -= 1
ELSE
	;通常射精時
	SIF E == 1
		CFLAG:PLAYER:24 = 3
	;大量射精時
	SIF E == 2
		CFLAG:PLAYER:24 = 5
	CFLAG:PLAYER:11 -= 1
ENDIF


@SAMEN_SHOOT_SOURCE
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;恋慕、親愛持ちで射精者が主人の場合は達成感のソースが増加する
IF (TALENT:3 || TALENT:6) && P == MASTER
	SOURCE:6 += 1500
ELSE
	SOURCE:8 += 1500
	SOURCE:18 += 500
ENDIF

;陥落素質が無い場合は反発増加
SIF TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0
	SOURCE:18 += 1000


