;-------------------------------------------------
;姫様訓練
;技能コマンド
;-------------------------------------------------
@COM599

PRINTL 姫様訓練
STR:0 = 姫様訓練
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費CP
LOSEBASE:0 = 2

;獲得基礎能力
SOURCE:姫様技能 = 100
;後の獲得能力は貞操によって異なる
IF BASE:貞操 < 100
	SOURCE:美麗 = 2
	SOURCE:淫妖 = 3
	
	TFLAG:美麗経験 = 1
	TFLAG:淫妖経験 = 2
ELSE
	SOURCE:美麗 = 3
	TFLAG:美麗経験 = 2
ENDIF

RETURN 1

