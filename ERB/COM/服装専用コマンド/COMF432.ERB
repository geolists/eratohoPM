;-------------------------------------------------
;淫行魔術
;服装固有コマンド
;ゴスロリ属性の服限定
;淫妖が200以上＆経験が150でないと使用できない
;-------------------------------------------------
@COM432

;消費CP
LOSEBASE:0 = 5

IF BASE:ＣＰ < LOSEBASE:0
	PRINTL CPが足りない
	RETURN 0
ENDIF

PRINTL 淫行魔術
STR:0 = 淫行魔術

CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

SOURCE:知力 = 12
SOURCE:淫妖 = 8
SOURCE:堕落 = 10

TFLAG:知力経験 = 2
TFLAG:淫妖経験 = 8

LOSEBASE:アライメント = -2

;穢れ加算処理
CALL CALC_IMP, 2

RETURN 1

