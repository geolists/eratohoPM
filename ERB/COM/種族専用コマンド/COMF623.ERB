@COM623
;消費CP
LOSEBASE:0 = 6

IF BASE:ＣＰ < LOSEBASE:0
	PRINTL CPが足りない
	RETURN 0
ENDIF

PRINTL スライムベッド
STR:0 = スライムベッド
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------


;獲得基礎能力
SOURCE:淫妖 = 15
SOURCE:魔力 = 30

TFLAG:淫妖経験 = 5

LOSEBASE:民衆の評価 += 2

;特殊属性
TFLAG:スライム = 1

RETURN 1

